{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=3072"
    }
  },
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}